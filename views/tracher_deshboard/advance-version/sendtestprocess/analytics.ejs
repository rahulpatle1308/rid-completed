<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Test Analytics Dashboard</title>
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.4.0/css/all.min.css">
  
  <style>
    /* ===== GLOBAL STYLES ===== */
    * {
      margin: 0;
      padding: 0;
      box-sizing: border-box;
      font-family: 'Segoe UI', system-ui, -apple-system, sans-serif;
    }
    
    body {
      background: linear-gradient(135deg, #0f172a 0%, #1e293b 100%);
      color: #f8fafc;
      min-height: 100vh;
      padding: 20px;
      position: relative;
      overflow-x: hidden;
    }
    
    /* Animated background elements */
    .bg-grid {
      position: fixed;
      top: 0;
      left: 0;
      width: 100%;
      height: 100%;
      background-image: 
        linear-gradient(rgba(30, 41, 59, 0.5) 1px, transparent 1px),
        linear-gradient(90deg, rgba(30, 41, 59, 0.5) 1px, transparent 1px);
      background-size: 40px 40px;
      z-index: -1;
      opacity: 0.3;
    }
    
    .floating-shapes {
      position: fixed;
      top: 0;
      left: 0;
      width: 100%;
      height: 100%;
      z-index: -1;
      overflow: hidden;
    }
    
    .shape {
      position: absolute;
      border-radius: 50%;
      background: linear-gradient(135deg, rgba(59, 130, 246, 0.1), rgba(168, 85, 247, 0.1));
      animation: float 15s infinite linear;
    }
    
    .shape:nth-child(1) {
      width: 300px;
      height: 300px;
      top: -150px;
      right: -150px;
      animation-delay: 0s;
    }
    
    .shape:nth-child(2) {
      width: 200px;
      height: 200px;
      bottom: -100px;
      left: -100px;
      animation-delay: 5s;
    }
    
    .shape:nth-child(3) {
      width: 150px;
      height: 150px;
      top: 50%;
      left: 10%;
      animation-delay: 10s;
    }
    
    @keyframes float {
      0%, 100% { transform: translate(0, 0) rotate(0deg); }
      33% { transform: translate(30px, 30px) rotate(120deg); }
      66% { transform: translate(-20px, 20px) rotate(240deg); }
    }
    
    /* ===== MAIN CONTAINER ===== */
    .dashboard-container {
      max-width: 1400px;
      margin: 0 auto;
      animation: fadeIn 0.8s ease-out;
    }
    
    @keyframes fadeIn {
      from { opacity: 0; transform: translateY(20px); }
      to { opacity: 1; transform: translateY(0); }
    }
    
    /* ===== HEADER STYLES ===== */
    .header {
      display: flex;
      justify-content: space-between;
      align-items: center;
      margin-bottom: 30px;
      padding: 20px;
      background: rgba(30, 41, 59, 0.7);
      border-radius: 16px;
      border: 1px solid rgba(100, 116, 139, 0.3);
      backdrop-filter: blur(10px);
      box-shadow: 0 10px 30px rgba(0, 0, 0, 0.2);
    }
    
    .header-content h2 {
      font-size: 28px;
      font-weight: 700;
      background: linear-gradient(90deg, #60a5fa, #a78bfa);
      -webkit-background-clip: text;
      background-clip: text;
      color: transparent;
      margin-bottom: 8px;
    }
    
    .header-content p {
      color: #cbd5e1;
      font-size: 15px;
      display: flex;
      align-items: center;
      gap: 8px;
    }
    
    .test-id-display {
      background: rgba(15, 23, 42, 0.8);
      padding: 12px 20px;
      border-radius: 12px;
      border: 1px solid rgba(59, 130, 246, 0.3);
    }
    
    .test-id-display span {
      color: #60a5fa;
      font-weight: 600;
      font-family: monospace;
      font-size: 16px;
    }
    
    /* ===== SEARCH BAR STYLES ===== */
    .search-container {
      background: rgba(30, 41, 59, 0.7);
      border-radius: 16px;
      border: 1px solid rgba(100, 116, 139, 0.3);
      backdrop-filter: blur(10px);
      box-shadow: 0 10px 30px rgba(0, 0, 0, 0.2);
      margin-bottom: 25px;
      padding: 20px;
      animation: slideUp 0.6s ease-out 0.2s both;
    }
    
    .search-header {
      display: flex;
      align-items: center;
      gap: 12px;
      margin-bottom: 15px;
    }
    
    .search-header i {
      font-size: 20px;
      color: #60a5fa;
    }
    
    .search-header h3 {
      font-size: 18px;
      color: #e2e8f0;
      font-weight: 600;
    }
    
    .search-box {
      position: relative;
      width: 100%;
    }
    
    .search-box i {
      position: absolute;
      left: 18px;
      top: 50%;
      transform: translateY(-50%);
      color: #94a3b8;
      font-size: 18px;
      z-index: 2;
    }
    
    .search-input {
      width: 100%;
      padding: 16px 20px 16px 50px;
      background: rgba(15, 23, 42, 0.8);
      border: 2px solid rgba(59, 130, 246, 0.2);
      border-radius: 12px;
      color: #f1f5f9;
      font-size: 16px;
      transition: all 0.3s ease;
    }
    
    .search-input:focus {
      outline: none;
      border-color: #3b82f6;
      box-shadow: 0 0 0 3px rgba(59, 130, 246, 0.1);
    }
    
    .search-input::placeholder {
      color: #94a3b8;
    }
    
    .search-stats {
      margin-top: 10px;
      display: flex;
      align-items: center;
      justify-content: space-between;
      color: #94a3b8;
      font-size: 14px;
      padding: 0 5px;
    }
    
    .search-results-count {
      background: rgba(59, 130, 246, 0.1);
      color: #60a5fa;
      padding: 4px 12px;
      border-radius: 20px;
      font-weight: 600;
      font-size: 13px;
    }
    
    /* ===== TABLE STYLES ===== */
    .table-container {
      background: rgba(30, 41, 59, 0.7);
      border-radius: 16px;
      border: 1px solid rgba(100, 116, 139, 0.3);
      backdrop-filter: blur(10px);
      box-shadow: 0 10px 30px rgba(0, 0, 0, 0.2);
      margin-bottom: 30px;
      overflow: hidden;
      animation: slideUp 0.6s ease-out 0.4s both;
    }
    
    @keyframes slideUp {
      from { opacity: 0; transform: translateY(40px); }
      to { opacity: 1; transform: translateY(0); }
    }
    
    table {
      width: 100%;
      border-collapse: separate;
      border-spacing: 0;
    }
    
    thead {
      background: linear-gradient(90deg, #1d4ed8, #7c3aed);
    }
    
    th {
      padding: 20px 16px;
      text-align: center;
      font-weight: 600;
      font-size: 16px;
      color: white;
      border-bottom: 2px solid rgba(255, 255, 255, 0.1);
      position: relative;
    }
    
    th:not(:last-child)::after {
      content: '';
      position: absolute;
      right: 0;
      top: 20%;
      height: 60%;
      width: 1px;
      background: rgba(255, 255, 255, 0.2);
    }
    
    tbody tr {
      transition: all 0.3s ease;
      border-bottom: 1px solid rgba(100, 116, 139, 0.2);
    }
    
    tbody tr:hover {
      background: rgba(59, 130, 246, 0.1);
      transform: translateY(-2px);
      box-shadow: 0 5px 15px rgba(0, 0, 0, 0.1);
    }
    
    td {
      padding: 18px 16px;
      text-align: center;
      border-bottom: 1px solid rgba(100, 116, 139, 0.1);
      transition: all 0.3s ease;
    }
    
    /* Rank column styling */
    td:first-child {
      font-weight: 700;
      font-size: 18px;
      color: #fbbf24;
      position: relative;
    }
    
    /* Gold, Silver, Bronze ranks */
    tr:nth-child(1) td:first-child::before {
      content: 'ðŸ¥‡';
      margin-right: 5px;
    }
    
    tr:nth-child(2) td:first-child::before {
      content: 'ðŸ¥ˆ';
      margin-right: 5px;
    }
    
    tr:nth-child(3) td:first-child::before {
      content: 'ðŸ¥‰';
      margin-right: 5px;
    }
    
    /* Status badges */
    td:last-child {
      font-weight: 600;
      border-radius: 20px;
      padding: 8px 16px;
      margin: 0 auto;
      display: inline-block;
      min-width: 100px;
    }
    
    /* Status colors */
    .status-passed {
      background: rgba(34, 197, 94, 0.15);
      color: #4ade80;
      border: 1px solid rgba(34, 197, 94, 0.3);
    }
    
    .status-failed {
      background: rgba(239, 68, 68, 0.15);
      color: #f87171;
      border: 1px solid rgba(239, 68, 68, 0.3);
    }
    
    .status-pending {
      background: rgba(245, 158, 11, 0.15);
      color: #fbbf24;
      border: 1px solid rgba(245, 158, 11, 0.3);
    }
    
    /* Loading state */
    .loading {
      padding: 60px 20px;
      text-align: center;
      font-size: 18px;
      color: #cbd5e1;
      background: rgba(30, 41, 59, 0.5);
      border-radius: 12px;
      margin: 10px;
    }
    
    .loading::after {
      content: '';
      display: inline-block;
      width: 20px;
      height: 20px;
      border: 3px solid rgba(59, 130, 246, 0.3);
      border-top-color: #3b82f6;
      border-radius: 50%;
      margin-left: 10px;
      animation: spin 1s linear infinite;
    }
    
    @keyframes spin {
      to { transform: rotate(360deg); }
    }
    
    /* No results state */
    .no-results {
      padding: 60px 20px;
      text-align: center;
      color: #94a3b8;
    }
    
    .no-results i {
      font-size: 48px;
      margin-bottom: 15px;
      color: #64748b;
    }
    
    /* ===== EXPORT SECTION ===== */
    .export-section {
      background: rgba(30, 41, 59, 0.7);
      border-radius: 16px;
      border: 1px solid rgba(100, 116, 139, 0.3);
      backdrop-filter: blur(10px);
      box-shadow: 0 10px 30px rgba(0, 0, 0, 0.2);
      padding: 25px 30px;
      margin-bottom: 30px;
      animation: slideUp 0.8s ease-out 0.6s both;
    }
    
    .export-section h3 {
      font-size: 22px;
      margin-bottom: 20px;
      display: flex;
      align-items: center;
      gap: 10px;
      color: #e2e8f0;
    }
    
    .export-section h3 i {
      color: #60a5fa;
    }
    
    .button-group {
      display: flex;
      flex-wrap: wrap;
      gap: 15px;
    }
    
    /* ===== BUTTON STYLES ===== */
    .btn {
      padding: 14px 28px;
      border: none;
      border-radius: 12px;
      font-weight: 600;
      font-size: 16px;
      cursor: pointer;
      display: inline-flex;
      align-items: center;
      justify-content: center;
      gap: 10px;
      transition: all 0.3s ease;
      min-width: 180px;
      position: relative;
      overflow: hidden;
    }
    
    .btn::before {
      content: '';
      position: absolute;
      top: 0;
      left: -100%;
      width: 100%;
      height: 100%;
      background: linear-gradient(90deg, transparent, rgba(255, 255, 255, 0.2), transparent);
      transition: left 0.7s;
    }
    
    .btn:hover::before {
      left: 100%;
    }
    
    .btn:hover {
      transform: translateY(-3px);
      box-shadow: 0 10px 20px rgba(0, 0, 0, 0.3);
    }
    
    .btn:active {
      transform: translateY(0);
    }
    
    .btn-back {
      background: linear-gradient(135deg, #475569, #334155);
      color: #f1f5f9;
      border: 1px solid #475569;
    }
    
    .btn-back:hover {
      background: linear-gradient(135deg, #4b5563, #374151);
    }
    
    .btn-pdf {
      background: linear-gradient(135deg, #ef4444, #dc2626);
      color: white;
      border: 1px solid #ef4444;
    }
    
    .btn-pdf:hover {
      background: linear-gradient(135deg, #f87171, #ef4444);
    }
    
    .btn-excel {
      background: linear-gradient(135deg, #10b981, #059669);
      color: white;
      border: 1px solid #10b981;
    }
    
    .btn-excel:hover {
      background: linear-gradient(135deg, #34d399, #10b981);
    }
    
    .btn-print {
      background: linear-gradient(135deg, #f59e0b, #d97706);
      color: white;
      border: 1px solid #f59e0b;
    }
    
    .btn-print:hover {
      background: linear-gradient(135deg, #fbbf24, #f59e0b);
    }
    
    /* ===== RESPONSIVE DESIGN ===== */
    @media (max-width: 1200px) {
      .dashboard-container {
        max-width: 95%;
      }
      
      .button-group {
        justify-content: center;
      }
    }
    
    @media (max-width: 992px) {
      .header {
        flex-direction: column;
        align-items: flex-start;
        gap: 20px;
      }
      
      .test-id-display {
        align-self: stretch;
        text-align: center;
      }
      
      .table-container {
        overflow-x: auto;
      }
      
      table {
        min-width: 900px;
      }
    }
    
    @media (max-width: 768px) {
      body {
        padding: 15px;
      }
      
      .header-content h2 {
        font-size: 24px;
      }
      
      .btn {
        min-width: 160px;
        padding: 12px 20px;
        font-size: 15px;
      }
      
      th, td {
        padding: 14px 10px;
        font-size: 14px;
      }
      
      .search-container, .export-section {
        padding: 15px;
      }
    }
    
    @media (max-width: 576px) {
      .header {
        padding: 15px;
      }
      
      .export-section {
        padding: 20px;
      }
      
      .button-group {
        flex-direction: column;
      }
      
      .btn {
        width: 100%;
      }
      
      th, td {
        padding: 12px 8px;
        font-size: 13px;
      }
      
      .search-input {
        padding: 14px 20px 14px 45px;
        font-size: 15px;
      }
    }
    
    /* ===== CUSTOM SCROLLBAR ===== */
    ::-webkit-scrollbar {
      width: 10px;
      height: 10px;
    }
    
    ::-webkit-scrollbar-track {
      background: rgba(30, 41, 59, 0.5);
      border-radius: 10px;
    }
    
    ::-webkit-scrollbar-thumb {
      background: linear-gradient(135deg, #60a5fa, #a78bfa);
      border-radius: 10px;
    }
    
    ::-webkit-scrollbar-thumb:hover {
      background: linear-gradient(135deg, #3b82f6, #8b5cf6);
    }
    
    /* ===== SEARCH HIGHLIGHT ===== */
    .highlight {
      background-color: rgba(251, 191, 36, 0.3);
      padding: 2px 4px;
      border-radius: 4px;
    }
  </style>
</head>

<body>
  <!-- Background Elements -->
  <div class="bg-grid"></div>
  <div class="floating-shapes">
    <div class="shape"></div>
    <div class="shape"></div>
    <div class="shape"></div>
  </div>
  
  <!-- Dashboard Container -->
  <div class="dashboard-container">
    
    <!-- Header -->
    <div class="header">
      <div class="header-content">
        <h2><i class="fas fa-chart-line"></i> Test Analytics Dashboard</h2>
        <p><i class="fas fa-info-circle"></i> Real-time student performance metrics and rankings</p>
      </div>
      <div class="test-id-display">
        <p>Test ID: <span id="testIdDisplay">Loading...</span></p>
      </div>
    </div>
     <!-- Export Section -->
    <div class="export-section">
      <h3><i class="fas fa-file-export"></i> Export All Student Rank Data</h3>
      <div class="button-group">
        <button class="btn btn-back" onclick="goBack()"><i class="fas fa-arrow-left"></i> Back to Dashboard</button>
        <button class="btn btn-pdf" onclick="exportPDF()"><i class="fas fa-file-pdf"></i> Export as PDF</button>
        <button class="btn btn-excel" onclick="exportExcel()"><i class="fas fa-file-excel"></i> Export as Excel</button>
        <button class="btn btn-print" onclick="printReport()"><i class="fas fa-print"></i> Print Report</button>
      </div>
    </div>
    <!-- Search Bar -->
    <div class="search-container">
      <div class="search-header">
        <i class="fas fa-search"></i>
        <h3>Search Students</h3>
      </div>
      <div class="search-box">
        <i class="fas fa-search"></i>
        <input type="text" class="search-input" id="searchInput" placeholder="Search by name, roll number, email, or parent contact...">
      </div>
      <div class="search-stats">
        <span id="searchHelp">Type to filter student records</span>
        <span id="searchResultsCount" class="search-results-count">0 results</span>
      </div>
    </div>
    
    <!-- Table Container -->
    <div class="table-container">
      <table>
        <thead>
          <tr>
            <th>Rank</th>
            <th>Name</th>
            <th>Roll No.</th>
            <th>Email</th>
            <th>Contact No.</th>
            <th>Marks</th>
            <th>Status</th>
          </tr>
        </thead>
        
        <tbody id="detailsBody">
          <tr>
            <td colspan="7" class="loading">Loading analytics data...</td>
          </tr>
        </tbody>
      </table>
    </div>
    
   
  </div>

<script>
  // âœ… GET testId from URL and display it
  const params = new URLSearchParams(window.location.search);
  const testId = params.get("testId");
  
  // Store original data for search functionality
  let originalData = [];
  let filteredData = [];
  
  if (testId) {
    document.getElementById('testIdDisplay').textContent = testId;
    loadAnalytics(testId);
  } else {
    document.getElementById("detailsBody").innerHTML =
      "<tr><td colspan='7' style='color: #f87171; padding: 40px; text-align: center;'>Test ID not found in URL</td></tr>";
  }
  
  // âœ… LOAD ANALYTICS DATA
  function loadAnalytics(testId) {
    fetch(`/api/teacher/analytics/test/${testId}/details`, {
      credentials: "include"
    })
    .then(res => {
      if (!res.ok) throw new Error('Network response was not ok');
      return res.json();
    })
    .then(data => {
      originalData = data;
      filteredData = [...originalData];
      
      if (!data || data.length === 0) {
        document.getElementById("detailsBody").innerHTML = `
          <tr>
            <td colspan="7" class="no-results">
              <i class="fas fa-inbox"></i>
              <h3>No Student Data Found</h3>
              <p>No analytics data available for this test.</p>
            </td>
          </tr>
        `;
        updateSearchResultsCount(0);
        return;
      }
      
      // Sort data by marks (descending) for ranking
     
      
      renderTable(data);
      updateSearchResultsCount(data.length);
      
      // Initialize search functionality
      initializeSearch();
    })
    .catch(err => {
      console.error(err);
      document.getElementById("detailsBody").innerHTML = `
        <tr>
          <td colspan="7" class="no-results">
            <i class="fas fa-exclamation-triangle"></i>
            <h3>Error Loading Data</h3>
            <p>Failed to load analytics data. Please try again later.</p>
          </td>
        </tr>
      `;
    });
  }
  
  // Render table with data
  function renderTable(data) {
    const tbody = document.getElementById("detailsBody");
    tbody.innerHTML = "";
    
    if (data.length === 0) {
      tbody.innerHTML = `
        <tr>
          <td colspan="7" class="no-results">
            <i class="fas fa-search"></i>
            <h3>No Matching Results</h3>
            <p>Try adjusting your search terms.</p>
          </td>
        </tr>
      `;
      return;
    }
    
    data.forEach((student, index) => {
      const row = document.createElement('tr');
      
      // Determine status color class
      let statusClass = 'status-pending';
      if (student.status === 'Passed' || student.status === 'Completed') {
        statusClass = 'status-passed';
      } else if (student.status === 'Failed' || student.status === 'Incomplete') {
        statusClass = 'status-failed';
      }
      
      row.innerHTML = `
        <td>${index + 1}</td>
        <td>${student.name || "-"}</td>
        <td>${student.roll || "-"}</td>
        <td style="color: #60a5fa;">${student.email || "-"}</td>
        <td>${student.parentContact || "-"}</td>
        <td style="font-weight: 700; color: #fbbf24;">${student.marks || 0}</td>
        <td class="${statusClass}">${student.status || "Pending"}</td>
      `;
      
      tbody.appendChild(row);
    });
    
    // Add animation to table rows
    animateTableRows();
  }
  
  // Initialize search functionality
  function initializeSearch() {
    const searchInput = document.getElementById('searchInput');
    const searchHelp = document.getElementById('searchHelp');
    
    // Focus on search input when page loads
    searchInput.focus();
    
    // Add event listener for real-time search
    searchInput.addEventListener('input', function() {
      performSearch(this.value.trim());
    });
    
    // Add keyboard shortcuts
    searchInput.addEventListener('keydown', function(e) {
      // Clear search on Escape key
      if (e.key === 'Escape') {
        this.value = '';
        performSearch('');
      }
      // Focus search on Ctrl/Cmd + F
      if ((e.ctrlKey || e.metaKey) && e.key === 'f') {
        e.preventDefault();
        this.focus();
      }
    });
    
    // Add search tips that change based on input
    searchInput.addEventListener('focus', function() {
      searchHelp.textContent = "Search by name, roll number, email, or parent contact...";
    });
    
    searchInput.addEventListener('blur', function() {
      if (this.value.trim() === '') {
        searchHelp.textContent = "";
      }
    });
  }
  
  // Perform search on the data
  function performSearch(searchTerm) {
    if (!searchTerm) {
      filteredData = [...originalData];
    } else {
      const term = searchTerm.toLowerCase();
      filteredData = originalData.filter(student => {
        return (
          (student.name && student.name.toLowerCase().includes(term)) ||
          (student.roll && student.roll.toLowerCase().includes(term)) ||
          (student.email && student.email.toLowerCase().includes(term)) ||
          (student.parentContact && student.parentContact.includes(term)) ||
          (student.status && student.status.toLowerCase().includes(term)) ||
          (student.marks && student.marks.toString().includes(term))
        );
      });
    }
    
    // Sort filtered data by marks
    filteredData.sort((a, b) => b.marks - a.marks);
    
    // Render filtered table
    renderTable(filteredData);
    
    // Update results count
    updateSearchResultsCount(filteredData.length);
    
    // Highlight search terms in table
    if (searchTerm) {
      highlightSearchTerms(searchTerm);
    }
  }
  
  // Update search results count display
  function updateSearchResultsCount(count) {
    const resultsCount = document.getElementById('searchResultsCount');
    const totalCount = originalData.length;
    
    if (count === totalCount) {
      resultsCount.textContent = `${count} students`;
    } else {
      resultsCount.textContent = `${count} of ${totalCount} students`;
    }
    
    // Change color based on results
    if (count === 0) {
      resultsCount.style.background = 'rgba(239, 68, 68, 0.1)';
      resultsCount.style.color = '#f87171';
    } else if (count < totalCount) {
      resultsCount.style.background = 'rgba(245, 158, 11, 0.1)';
      resultsCount.style.color = '#fbbf24';
    } else {
      resultsCount.style.background = 'rgba(59, 130, 246, 0.1)';
      resultsCount.style.color = '#60a5fa';
    }
  }
  
  // Highlight search terms in table
  function highlightSearchTerms(term) {
    const rows = document.querySelectorAll('#detailsBody tr:not(.no-results)');
    const regex = new RegExp(`(${term})`, 'gi');
    
    rows.forEach(row => {
      const cells = row.querySelectorAll('td');
      cells.forEach((cell, index) => {
        // Skip rank column (index 0) and marks column (index 5)
        if (index === 0 || index === 5 || index === 6) return;
        
        const originalText = cell.textContent;
        const highlightedText = originalText.replace(regex, '<span class="highlight">$1</span>');
        if (highlightedText !== originalText) {
          cell.innerHTML = highlightedText;
        }
      });
    });
  }
  
  // Animate table rows on load
  function animateTableRows() {
    const rows = document.querySelectorAll('#detailsBody tr:not(.no-results)');
    rows.forEach((row, index) => {
      row.style.animationDelay = `${index * 0.05}s`;
      row.style.animation = 'fadeIn 0.5s ease-out forwards';
      row.style.opacity = '0';
    });
  }
  
  // âœ… Navigation and Export Functions
  function goBack() {
    // Add a nice transition effect
    document.querySelector('.dashboard-container').style.animation = 'fadeIn 0.5s ease-out reverse forwards';
    setTimeout(() => {
      window.location.href = "/teacher-dashboard";
    }, 300);
  }
  
  function exportPDF() {
    if (!testId) {
      alert("Test not selected");
      return;
    }
    // Show loading state on button
    const btn = document.querySelector('.btn-pdf');
    const originalHTML = btn.innerHTML;
    btn.innerHTML = '<i class="fas fa-spinner fa-spin"></i> Generating PDF...';
    btn.disabled = true;
    
    setTimeout(() => {
      window.open(`/api/teacher/analytics/test/${testId}/export/pdf`);
      btn.innerHTML = originalHTML;
      btn.disabled = false;
    }, 1000);
  }
  
  function exportExcel() {
    if (!testId) {
      alert("Test not selected");
      return;
    }
    // Show loading state on button
    const btn = document.querySelector('.btn-excel');
    const originalHTML = btn.innerHTML;
    btn.innerHTML = '<i class="fas fa-spinner fa-spin"></i> Generating Excel...';
    btn.disabled = true;
    
    setTimeout(() => {
      window.open(`/api/teacher/analytics/test/${testId}/export/excel`);
      btn.innerHTML = originalHTML;
      btn.disabled = false;
    }, 1000);
  }
  
  function printReport() {
    // Add print-specific styling
    const printStyle = document.createElement('style');
    printStyle.textContent = `
      @media print {
        body * {
          visibility: hidden;
        }
        .dashboard-container, .dashboard-container * {
          visibility: visible;
        }
        .dashboard-container {
          position: absolute;
          left: 0;
          top: 0;
          width: 100%;
          box-shadow: none;
          background: white;
          color: black;
        }
        .btn, .floating-shapes, .bg-grid, .search-container {
          display: none !important;
        }
        .table-container {
          box-shadow: none;
          border: 1px solid #ccc;
        }
        th {
          background: #2c3e50 !important;
          -webkit-print-color-adjust: exact;
          color-adjust: exact;
        }
        .highlight {
          background-color: #ffff00 !important;
          color: black !important;
        }
      }
    `;
    document.head.appendChild(printStyle);
    
    window.print();
    
    // Remove the print style after printing
    setTimeout(() => {
      document.head.removeChild(printStyle);
    }, 100);
  }
  
  // Add interactive effects on page load
  document.addEventListener('DOMContentLoaded', function() {
    // Add hover effect to buttons
    const buttons = document.querySelectorAll('.btn');
    buttons.forEach(btn => {
      btn.addEventListener('mouseenter', function() {
        this.style.transform = 'translateY(-3px)';
      });
      
      btn.addEventListener('mouseleave', function() {
        this.style.transform = 'translateY(0)';
      });
    });
    
    // Add click effect to buttons
    buttons.forEach(btn => {
      btn.addEventListener('mousedown', function() {
        this.style.transform = 'translateY(0)';
      });
      
      btn.addEventListener('mouseup', function() {
        this.style.transform = 'translateY(-3px)';
      });
    });
  });
</script>
</body>
